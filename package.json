{
	"name": "@sapphire/template",
	"version": "0.0.1",
	"description": "template",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "@sapphire",
	"license": "MIT",
	"private": false,
	"scripts": {
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write {src,tests}/**/*.ts",
		"docs": "typedoc",
		"test": "jest",
		"test:watch": "jest --watch",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepublishOnly": "yarn build",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.2.1",
		"@commitlint/config-conventional": "^13.2.0",
		"@sapphire/eslint-config": "^3.3.1",
		"@sapphire/prettier-config": "^1.2.1",
		"@sapphire/ts-config": "^3.1.0",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.9.1",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.2",
		"jest": "^27.2.5",
		"jest-circus": "^27.2.5",
		"lint-staged": "^11.2.2",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1",
		"standard-version": "^9.3.1",
		"ts-jest": "^27.0.5",
		"ts-node": "^10.2.1",
		"typedoc": "^0.22.5",
		"typescript": "^4.4.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/template.git"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"engines": {
		"node": ">=v14.18.0",
		"npm": ">=7.24.2"
	},
	"keywords": [
		"@sapphire/template",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/template/issues"
	},
	"homepage": "https://sapphiredev.github.io/template",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"minimist": "^1.2.5",
		"kind-of": "^6.0.3",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"dot-prop": "^6.0.1",
		"lodash": "^4.17.21",
		"marked": "^3.0.7",
		"merge": "^2.1.1",
		"trim": "^1.0.1",
		"trim-newlines": "^4.0.2"
	},
	"prettier": "@sapphire/prettier-config"
}
